<?xml version='1.0' encoding='utf-8'?> 
<Context displayName="WimeBackstage" docBase="WimeBackstage" path="/WimeBackstage" reloadable="true">
  <!--                     MySQL                   -->
  <Resource name="jdbc/WIME" type="javax.sql.DataSource"
      username="root" password="mysql"
      driverClassName="com.mysql.jdbc.Driver"
      url="jdbc:mysql:///wime"
      connectionProperties="useUnicode=yes;characterEncoding=utf8;"
      maxActive="8"
      maxIdle="4"
      maxWait="5000"
  />  
</Context>
